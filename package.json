{
  "name": "vyamikk-samadhaan-new",
  "main": "index.js",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev": "expo start -c",
    "dev:api": "cd server && node index.js",
    "dev:tunnel": "lt --port 4000 --print-requests",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "doctor": "bash -lc 'npx kill-port 8081 19000 19001 19002 19006 4000 || true; rm -rf .expo .expo-shared || true; rm -rf $TMPDIR/metro-* 2>/dev/null || true'",
    "api": "DEV_AUTH_ANY=1 node api/server.js",
    "start:lan": "npm run doctor && npx expo start -c --lan",
    "start:tunnel": "npm run doctor && EXPO_TUNNEL_SUBDOMAIN=vyamikk npx expo start -c --tunnel",
    "start:usb": "npm run doctor && adb start-server >/dev/null 2>&1 || true && adb devices && adb reverse tcp:8081 tcp:8081 && adb reverse tcp:19000 tcp:19000 && adb reverse tcp:19001 tcp:19001 && adb reverse tcp:4000 tcp:4000 && EXPO_PUBLIC_API_MODE=usb npx expo start -c --offline"
  },
  "dependencies": {
    "@expo-google-fonts/inter": "^0.4.1",
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.26",
    "@tanstack/react-query": "^5.87.4",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "expo": "~53.0.22",
    "expo-blur": "~14.1.5",
    "expo-constants": "~17.1.7",
    "expo-font": "~13.3.2",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.4.0",
    "expo-linking": "~7.1.7",
    "expo-location": "~18.1.0",
    "expo-router": "~5.1.5",
    "expo-secure-store": "~14.2.4",
    "expo-splash-screen": "^0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.11",
    "expo-web-browser": "~14.2.0",
    "ioredis": "^5.7.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^4.1.2",
    "react-native": "0.79.6",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/node": "^24.3.1",
    "@types/react": "~19.0.10",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "express-rate-limit": "^7.5.1",
    "helmet": "^7.2.0",
    "isomorphic-dompurify": "^2.26.0",
    "kill-port": "^2.0.1",
    "typescript": "~5.8.3",
    "validator": "^13.15.15"
  },
  "private": true
}
