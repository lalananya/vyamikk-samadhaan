                                                "edit": "<%= entity.name %> संपादित करें",
                                                    "delete": "<%= entity.name %> हटाएं",
                                                        "confirm_delete": "<%= entity.name %> हटाएं?",
                                                            "confirm_delete_message": "यह क्रिया पूर्ववत नहीं की जा
                                                            सकती।"
                                                            },
                                                            <% }); %>
                                                                <% } %>
